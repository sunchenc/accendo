        <script type="text/javascript" language="javascript" >
            function computeminutes(index)
            {
                var enddate = $('#end_date_' + index).val();
                if (enddate.length == 0) {
                    $('#end_date_' + index).val($('#start_date_' + index).val());
                    check_change('end_date_' + index);
                }
                var modifierunits = $('#modifier_unit_' + index).val();
                if (modifierunits == null || modifierunits == "")
                    modifierunits = 0;
                var s1 = $('#start_date_' + index).val() + ' ' + $('#start_time_' + index).val();
                var s2 = $('#end_date_' + index).val() + " " + $('#end_time_' + index).val();
                var t2 = $('#end_time_' + index).val();
                var start = new Date(Date.parse(s1.replace(/-/g, "/")));
                var end = new Date(Date.parse(s2.replace(/-/g, "/")));
                var time = Math.ceil((end.getTime() - start.getTime()) / 60 / 1000);
                var crosswalkcode = $('#secondary_CPT_code_' + index).val();
                var provider_id = $('#provider_id').val();
                var cpt_code = $('#CPT_code_' + index).val();
                var units;
                var roundup;
                //alert("time = " + time);
                $.post("<?php echo $this->baseUrl ?>/biller/claims/crosswalkcode/", {crosswalkcode: crosswalkcode, provider_id: provider_id, cpt_code: cpt_code},
                function (data) {
                    var myjson = '';
                    eval("myjson=" + data + ";");
                    units = myjson.base_unit;
                    roundup = myjson.roundup;

                    //alert('units = ' + units + ' computerminutes::roundup = '+ roundup);
                    //if($('#start_date_'+index).val()!=""&&$('#end_date_'+index).val()!=""&&$('#start_time_'+index).val()!=""&&$('#end_time_'+index).val()!="")
                    if (time > 0)
                    {
                        var toomanyminutes_val = parseInt($('#toomanyminutes').val());
                        if (time <= toomanyminutes_val) {
                            $('#minutes_' + index).val(time);
                            if (units.length > 0)
                                if (roundup > 0)
                                    $('#days_or_units_' + index).val(Math.ceil(time / 15) + parseInt(units) + parseInt(modifierunits));
                                else
                                    $('#days_or_units_' + index).val(Math.round((time / 15) * 10) / 10 + parseInt(units) + parseInt(modifierunits));
                            else
                            if (roundup > 0)
                                $('#days_or_units_' + index).val(Math.ceil(time / 15) + parseInt(modifierunits));
                   //     (time - time%15)/15 + time%15 >= roundup: 1: 0
                            else
                                $('#days_or_units_' + index).val(Math.round((time / 15) * 10) / 10 + parseInt(modifierunits));
                        } else {
                            $('#minutes_' + index).val(time);
                            if (units.length > 0)
