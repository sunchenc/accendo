                        }else{
                            bill_status_display.style.borderColor="#ffffff";
                          $("#bexisting").hide();
                        }
                        //$('#modifier_1').val(myjson.modifier_1);
                        //$('#modifier_2').val(myjson.modifier_2);
                    
                 
                });
            }
