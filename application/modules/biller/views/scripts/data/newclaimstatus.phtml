                function(data){
                    var myjson='';
                    eval("myjson=" + data + ";");
                    var bill_status_display = document.getElementById("bill_status_display");
                    bill_status_display.style.borderColor="#ffffff";
                        $("#bexisting").hide();
                        var myexist=myjson.existing;
