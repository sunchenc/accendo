        <script type="text/javascript" language="Javascript" >
            function mydisplay(name)
            {
                var status=$(name).val();
                if(status.length == 1){
                    if(status[0]=='i'||status[0]=='I')
                        $(name).val('Inactive-');
                    else if(status[0]=='o'||status[0]=='O')
                        $(name).val('Open-');
                    else if(status[0]=='c'||status[0]=='C')
                        $(name).val('Closed-');
                    else
                        $(name).val('');
                } else{
    
                    //var claimstatus =$('#claim_status').val();
 
                    var claimstatusdisplay =$('#claim_status_display').val();
 
                    if(claimstatusdisplay.length>0){
                        if(claimstatusdisplay!='open'&&claimstatusdisplay!='closed'&&claimstatusdisplay!='inactive'){
